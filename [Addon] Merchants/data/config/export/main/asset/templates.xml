<ModOps>
  <!--Trading Company-->
  <ModOp Type="addNextSibling" Template="PublicServiceBuilding">
    <Template>
      <Name>DrakkamMonument</Name>
      <Properties>
        <Standard>
          <InfoDescription>100027</InfoDescription>
        </Standard>
        <Building>
          <BuildingType>Factory</BuildingType>
          <BuildingCategoryName>11150</BuildingCategoryName>
        </Building>
        <Blocking/>
        <Cost/>
        <Object>
          <IgnoreForMinimapGeneration>1</IgnoreForMinimapGeneration>
        </Object>
        <Mesh/>
        <Selection>
          <GUIType>Production</GUIType>
          <OnSelection>
            <IsBaseAutoCreateAsset>1</IsBaseAutoCreateAsset>
            <Values>
              <ActionList/>
            </Values>
          </OnSelection>
        </Selection>
        <Text/>
        <Constructable>
          <DestructionCategory>Production</DestructionCategory>
        </Constructable>
        <Locked/>
        <SoundEmitter>
          <DestroySounds>
            <Item>
              <Sound>9818756</Sound>
              <!--wwe_sfx_aui_delete-->
            </Item>
          </DestroySounds>
          <ConstructionSounds>
            <BuildFailed>
              <Item>
                <Sound>9818387</Sound>
                <!--wwe_sfx_aui_deny-->
              </Item>
            </BuildFailed>
            <BuildMoveStart>
              <Item>
                <Sound>9818755</Sound>
                <!--wwe_sfx_aui_move-->
              </Item>
            </BuildMoveStart>
            <BuildMoveSuccess>
              <Item>
                <Sound>203995</Sound>
                <!--wwe_sfx_aui_move_replace_stone-->
              </Item>
            </BuildMoveSuccess>
            <BuildMoveEnd>
              <Item>
                <Sound>200141</Sound>
                <!--wwe_sfx_aui_move_end-->
              </Item>
            </BuildMoveEnd>
          </ConstructionSounds>
        </SoundEmitter>
        <FeedbackController>
          <NightBehavior>HideNone</NightBehavior>
          <StopOldSequenceWhenStartingNewOne>1</StopOldSequenceWhenStartingNewOne>
        </FeedbackController>
        <Infolayer/>
        <FactoryBase/>
        <LogisticNode/>
        <Culture>
          <CultureType>Dockland</CultureType>
        </Culture>
        <AmbientMoodProvider/>
        <Maintenance>
          <IncomeCategory>CultureBuildings</IncomeCategory>
        </Maintenance>
        <Attackable/>
        <Pausable/>
        <BuildingUnique/>
        <UpgradeList/>
        <Industrializable>
          <BoostedByIndustrialization>0</BoostedByIndustrialization>
        </Industrializable>
        <QuestObject/>
        <Electrifiable/>
      </Properties>
    </Template>
  </ModOp>

  <!--Train Stations-->
  <ModOp Type="addNextSibling" Template="PublicServiceBuilding">
    <Template>
      <Name>CentralTrainStation</Name>
      <Properties>
        <Standard/>
        <Text/>
        <Blocking/>
        <Building>
          <BuildingType>Logistic</BuildingType>
          <DestructionConfirmation>1</DestructionConfirmation>
        </Building>
        <Cost/>
        <Selection>
          <GUIType>Airport</GUIType>
          <OnSelection>
            <IsBaseAutoCreateAsset>1</IsBaseAutoCreateAsset>
            <Values>
              <ActionList/>
            </Values>
          </OnSelection>
          <ParticipantMessageTrigger>ClickKontor</ParticipantMessageTrigger>
          <AllowedDragType>ItemSocket;CargoProductStack;CargoItemStack</AllowedDragType>
          <ParticipantMessageArcheType>[NONE]</ParticipantMessageArcheType>
        </Selection>
        <Object>
          <IgnoreForMinimapGeneration>1</IgnoreForMinimapGeneration>
        </Object>
        <Constructable>
          <DestructionCategory>Production</DestructionCategory>
        </Constructable>
        <Mesh/>
        <SoundEmitter>
          <DestroySounds>
            <Item>
              <Sound>9818756</Sound>
              <!--wwe_sfx_aui_delete-->
            </Item>
          </DestroySounds>
          <ConstructionSounds>
            <BuildSuccessful>
              <Item>
                <VectorElement>
                  <InheritedIndex>0</InheritedIndex>
                </VectorElement>
                <Sound>214784</Sound>
                <!--wwe_sfx_aui_bld_place_harbor-->
              </Item>
            </BuildSuccessful>
            <BuildFailed>
              <Item>
                <Sound>9818387</Sound>
                <!--wwe_sfx_aui_deny-->
              </Item>
            </BuildFailed>
            <BuildMoveStart>
              <Item>
                <Sound>9818755</Sound>
                <!--wwe_sfx_aui_move-->
              </Item>
            </BuildMoveStart>
            <BuildMoveSuccess>
              <Item>
                <Sound>203995</Sound>
                <!--wwe_sfx_aui_move_replace_stone-->
              </Item>
            </BuildMoveSuccess>
            <BuildMoveEnd>
              <Item>
                <Sound>200141</Sound>
                <!--wwe_sfx_aui_move_end-->
              </Item>
            </BuildMoveEnd>
          </ConstructionSounds>
        </SoundEmitter>
        <Locked/>
        <Infolayer/>
        <FeedbackController>
          <NightBehavior>HideNone</NightBehavior>
        </FeedbackController>
        <Warehouse/>
        <LogisticNode/>
        <AmbientMoodProvider/>
        <StorageBase/>
        <Attackable>
          <RuinSmokeParticles>
            <Item>
              <SmokeParticleFilePath>data/graphics/effects/military/explosions/damage_smoke_01.cfg</SmokeParticleFilePath>
            </Item>
          </RuinSmokeParticles>
          <RuinSmokeParticlesMin>1</RuinSmokeParticlesMin>
          <RuinSmokeParticlesMax>3</RuinSmokeParticlesMax>
          <RuinSmokeParticleRectBorderPercent>0.2</RuinSmokeParticleRectBorderPercent>
          <ExplosionEffects>
            <Item/>
          </ExplosionEffects>
          <SelfHealPerHealTick>300</SelfHealPerHealTick>
        </Attackable>
        <Maintenance>
          <IncomeCategory>InfrastructureBuildings</IncomeCategory>
        </Maintenance>
        <Pausable/>
        <ModuleOwner/>
        <UpgradeList/>
        <TrainStation>
          <MaxTrainCount>3</MaxTrainCount>
        </TrainStation>
      </Properties>
    </Template>
  </ModOp>
</ModOps>